#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/noriken/Desktop/02_仕事_コード/01_TASK AI/task-mvp/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.20.0/node_modules/@openapitools/openapi-generator-cli/node_modules:/Users/noriken/Desktop/02_仕事_コード/01_TASK AI/task-mvp/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.20.0/node_modules/@openapitools/node_modules:/Users/noriken/Desktop/02_仕事_コード/01_TASK AI/task-mvp/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.20.0/node_modules:/Users/noriken/Desktop/02_仕事_コード/01_TASK AI/task-mvp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/noriken/Desktop/02_仕事_コード/01_TASK AI/task-mvp/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.20.0/node_modules/@openapitools/openapi-generator-cli/node_modules:/Users/noriken/Desktop/02_仕事_コード/01_TASK AI/task-mvp/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.20.0/node_modules/@openapitools/node_modules:/Users/noriken/Desktop/02_仕事_コード/01_TASK AI/task-mvp/node_modules/.pnpm/@openapitools+openapi-generator-cli@2.20.0/node_modules:/Users/noriken/Desktop/02_仕事_コード/01_TASK AI/task-mvp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@openapitools/openapi-generator-cli/main.js" "$@"
else
  exec node  "$basedir/../@openapitools/openapi-generator-cli/main.js" "$@"
fi
